! Emacs (for emacsclient; needs to be same as in .emacs)
emacs.font: Ubuntu Mono-9

! XTerm
xterm*faceName: Ubuntu Mono
xterm*faceSize: 9

xterm*internalBorder:        0
xterm*scrollBar:         false
xterm*cursorBlink:       false
xterm*bellIsUrgent:	 true
!xterm*geometry:         80x26
xterm*saveLines:         65535
xterm*dynamicColors:        on
xterm*highlightSelection: true
! Appearance
xterm*utf8:            2
xterm*eightBitInput:   true
xterm*metaSendsEscape: true
xterm*cursorColor: #DCDCCC
! Zenburn
xterm*background:  #3F3F3F
xterm*foreground:  #DCDCCC
!black
xterm*color0:      #000000
!red
xterm*color1:      #CC9393
!green
xterm*color2:      #7F9F7F
!yellow
xterm*color3:      #F0DFAF
!blue
xterm*color4:      #94BFF3
!magenta
xterm*color5:      #DC8CC3
!cyan
xterm*color6:      #93E0E3
!white
xterm*color7:      #DCDCCC
!bright black
xterm*color8:      #6F6F6F
!bright red
xterm*color9:      #DCA3A3
!bright green
xterm*color10:     #8FB28F
!bright yellow
xterm*color11:     #F0DFAF
!bright blue
xterm*color12:     #94BFF3
!bright magenta
xterm*color13:     #DC8CC3
!bright cyan
xterm*color14:     #93E0E3
!bright white
xterm*color15:     #FFFFEF

XLock.qix.delay:	20000
XLock.qix.batchcount:	32
XLock.qix.saturation:	1
XLock.mode:	qix
XLock.background:	black
XLock.foreground:	red
